            <field name="is_need_access_token" eval="True"/>
            <field name="headers">'{"Cache-Control": "no-cache"}'</field>
        </record>

        <record id="ghtk_print_order_route" model="delivery.route.api">
            <field name="provider_id" ref="tangerine_delivery_ghtk.tangerine_delivery_ghtk_provider" />
            <field name="name">Print Order</field>
            <field name="code">ghtk_print_order</field>
            <field name="route">/services/label</field>
            <field name="method">GET</field>
            <field name="is_need_access_token" eval="True"/>
            <field name="headers">'{"Cache-Control": "no-cache"}'</field>
        </record>
    </data>
