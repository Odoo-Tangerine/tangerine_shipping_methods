        <record id="lalamove_get_quotation" model="delivery.route.api">
            <field name="provider_id" ref="tangerine_delivery_lalamove.tangerine_delivery_lalamove_provider" />
            <field name="name">Get Quotation</field>
            <field name="code">llm_get_quotation</field>
            <field name="route">/v3/quotations</field>
            <field name="method">POST</field>
            <field name="headers">'{"Cache-Control": "no-cache", "Content-Type": "application/json"}'</field>
            <field name="is_need_access_token" eval="True"/>
        </record>

